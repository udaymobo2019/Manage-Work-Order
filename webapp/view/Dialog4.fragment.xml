<core:FragmentDefinition xmlns:core="sap.ui.core" controllerName="com.sap.build.ba293bd41-us_1.manageWorkOrder.controller.Dialog4"
	xmlns="sap.m" xmlns:form="sap.ui.layout.form">
	<Dialog contentWidth="auto" horizontalScrolling="true" icon="" showHeader="true" state="None" stretch="false"
